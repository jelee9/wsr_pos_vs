<Window x:Class="wsr_pos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wsr_pos"
        mc:Ignorable="d"
        Title="MainWindow" 
		Top="0"
		Left="0"
		Width="1280"
		Height="800"
		WindowStyle="None"
		ResizeMode="NoResize">
	<Canvas Name="canvas">
		<Label Name="label" FontSize="30">이종은 HelloWorld</Label>
		<Button Name="button" Canvas.Top="100" Canvas.Left="50" Width="100" Height="50" Click="button_click">hahaha</Button>
	</Canvas>
</Window>
